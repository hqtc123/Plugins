{
  "manifest_version": 2,
  "name": "alert2floor",
  "description": "get nju tieba 2nd floor",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://tieba.baidu.com/*"
  ],
  "browser_action": {
    "default_icon": "sofa.png",
    "default_title": "去抢2楼",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "sofa.png"
  },
  "content_scripts": [
    {
      "matches": ["http://tieba.baidu.com/*"],
      "js": [
        "jquery-2.1.3.min.js",
        "detect.js"
      ]
    }
  ]
}